<h1>Instructor Login</h1>
<%= form_with url: login_path do |f| %>
    <%= f.label :Username %>
    <%= f.text_field :username %>
    <br>
    <%= f.label :Password %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit 'Log In' %>
<% end %>

<%= link_to "Instructor Signup", signup_path %>
<br>
<%= link_to "Polyglot", root_path %>